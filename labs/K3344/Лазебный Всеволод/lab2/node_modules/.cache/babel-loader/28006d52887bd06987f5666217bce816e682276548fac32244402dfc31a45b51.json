-radius: 60% / 10%;\r\n  transform: translateY(-180px);\r\n  transition: 1s ease-in-out;\r\n}\r\n\r\n.login label{\r\n  color: #0d6dfb;\r\n  font-size: 2.3em;\r\n  justify-content: center;\r\n  display: flex;\r\n  padding-top:10px;\r\n  margin-top: 60px;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n}\r\n\r\n#chk:checked ~ .login{\r\n  transform: translateY(-570px);\r\n}\r\n\r\n#chk:checked ~ .login label{\r\n  transform: scale(1);\r\n}\r\n\r\n#chk:checked ~ .signup label{\r\n  transform: scale(1);\r\n  transition: 1s ease-in-out;\r\n}\r\n\r\n.social {\r\n  text-align: center; /* Центрируем текст */\r\n  margin-top: 40px; /* Отступ сверху */\r\n}\r\n\r\n.social-icon {\r\n  color: #212529; /* Изменяем цвет значков на синий */\r\n  font-size: 24px; /* Размер значков */\r\n  margin: 0 10px; /* Отступы между значками */\r\n  text-decoration: none; /* Убираем подчеркивание */\r\n}\r\n\r\n.social-icon:hover {\r\n  color: #0d6cf9;\r\n}\r\n</style>"],"mappings":"AACA,SAASA,GAAG,EAAEC,SAAS,EAAEC,eAAe,QAAQ,KAAK;;AAErD;;;;;;;;IACA,MAAMC,IAAI,GAAGH,GAAG,CAAC,EAAE,CAAC;IACpB,MAAMI,KAAK,GAAGJ,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMK,QAAQ,GAAGL,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMM,UAAU,GAAGN,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMO,aAAa,GAAGP,GAAG,CAAC,EAAE,CAAC;;IAE7B;IACA,MAAMQ,YAAY,GAAGA,CAAA,KAAM;MACzB;IAAA,CACD;;IAED;IACA,MAAMC,SAAS,GAAGA,CAAA,KAAM;MACtB;IAAA,CACD;;IAED;IACA,MAAMC,YAAY,GAAGA,CAAA,KAAM;MACzBC,QAAQ,CAACC,IAAI,CAACC,SAAS,CAACC,GAAG,CAAC,YAAY,CAAC;IAC3C,CAAC;;IAED;IACA,MAAMC,eAAe,GAAGA,CAAA,KAAM;MAC5BJ,QAAQ,CAACC,IAAI,CAACC,SAAS,CAACG,MAAM,CAAC,YAAY,CAAC;IAC9C,CAAC;;IAED;IACAf,SAAS,CAACS,YAAY,CAAC;;IAEvB;IACAR,eAAe,CAACa,eAAe,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}