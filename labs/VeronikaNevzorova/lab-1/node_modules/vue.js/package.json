  "scripts": {
    "[COMMON]": "====== Common shortcuts =======",
    "clean": "rimraf dist",
    "coverage": "istanbul cover src",
    "lint": "eslint --ext=.js,.vue src test",
    "patch": "npm version patch",
    "postpatch": "npm run pushtags",
    "minor": "npm version minor",
    "postminor": "npm run pushtags",
    "major": "npm version major",
    "postmajor": "npm run pushtags",
    "pushtags": "git push --tags && npm publish",
    "[DEVELOP]": "====== Dev server with hotreload supports for Vue =======",
    "start": "webpack-dev-server --config etc/webpack.develop.js",
    "[STAGING]": "======= [PM2] Staging server with static assets supports =======",
    "start:staging": "webpack --config etc/webpack.staging.js",
    "poststop:staging": "npm run clean",
    "[RELEASE]": "======= [PM2] Production server =======",
    "start:release": "webpack --config etc/webpack.release.js",
    "poststop:release": "npm run clean",
    "[TEST]": "======= Test case runner suites =======",
    "pretest": "npm run lint",
    "test": "mocha --require ignore-styles --compilers js:babel-core/register --recursive test/**/*.spec.js src/**/*.spec.js"
  },
